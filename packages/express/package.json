  },
  "engines": {
    "node": ">= 14"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "src/**",
    "esm/**",
    "lib/**",
    "public/**"
  ],
  "scripts": {
    "prepublish": "npm run compile",
    "pack": "npm pack --pack-destination ../cli/test/build",
    "compile:esm": "shx rm -rf esm/ && tsc --target es2020 --module es2020 --outDir esm/ --moduleResolution node",
    "compile:lib": "shx rm -rf lib/ && tsc --outDir lib/",
    "compile": "npm run compile:lib && npm run compile:esm && tsc && npm run pack",
    "test": "mocha --config ../../.mocharc.json --recursive test/**.test.ts test/**/*.test.ts"
  },
  "directories": {
    "lib": "lib"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@feathersjs/authentication": "^5.0.0-pre.32",
