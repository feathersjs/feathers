{
  "name": "@feathersjs/feathers",
  "description": "A framework for real-time applications and REST API with JavaScript and TypeScript",
  "version": "4.5.15",
  "homepage": "http://feathersjs.com",
  "repository": {
    "type": "git",
    "url": "git://github.com/feathersjs/feathers.git"
  },
  "keywords": [
    "feathers",
    "REST",
    "socket.io",
    "realtime"
  ],
  "main": "lib/index.js",
  "types": "index.d.ts",
  "author": {
    "name": "Feathers contributors",
    "email": "hello@feathersjs.com",
    "url": "https://feathersjs.com"
  },
  "license": "MIT",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/daffl"
  },
  "bugs": {
    "url": "https://github.com/feathersjs/feathers/issues"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "src/**",
    "lib/**",
    "*.d.ts",
    "*.js"
  ],
  "scripts": {
    "test": "mocha --config ../../.mocharc.json",
    "write-version": "node -e \"console.log('module.exports = \\'' + require('./package.json').version + '\\';')\" > lib/version.js",
    "reset-version": "node -e \"console.log('module.exports = \\'development\\';')\" > lib/version.js",
    "version": "npm run write-version",
    "publish": "npm run reset-version"
  },
  "engines": {
    "node": ">= 10"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@feathersjs/commons": "^4.5.15",
    "debug": "^4.3.3",
    "events": "^3.3.0",
    "uberproto": "^2.0.6"
  },
  "devDependencies": {
    "mocha": "^9.1.3"
  },
  "gitHead": "d0e9600002975296ab8de926acd317d0a73fce5c"
