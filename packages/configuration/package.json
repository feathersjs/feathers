{
  "name": "@feathersjs/configuration",
  "description": "A small configuration module for your Feathers application.",
  "version": "5.0.0-pre.2",
  "homepage": "https://feathersjs.com",
  "main": "lib/",
  "types": "lib/",
  "keywords": [
    "feathers",
    "feathers-plugin"
  ],
  "license": "MIT",
  "funding": {
    "type": "github",
