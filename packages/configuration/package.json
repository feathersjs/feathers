  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/feathersjs/feathers/issues"
  },
  "engines": {
    "node": ">= 12"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "src/**",
    "lib/**",
    "*.d.ts",
    "*.js"
