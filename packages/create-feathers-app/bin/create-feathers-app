#!/usr/bin/env node
'use strict';

const { comandRunner } = require('@feathersjs/cli')

export const program = new Command()

program
  .name('create-feathers-app')
  .description('Create a new Feathers application 🕊️')
  .argument('<name>', 'The name of your new application')
  // .version(version)
  .showHelpAfterError()
  .action((name, options) => {
    console.log(name, options)
  })